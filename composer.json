{
  "name": "tjsteinhaus/netsuite-php",
  "description": "NetSuite PHP API wrapper orginally built by Ryan Winchester.",
  "keywords": [ "netsuite", "api", "php" ],
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
	{
		"name": "Tyler Steinhaus",
		"email": "tjsteinhaus@gmail.com"
	},
	{
		"name": "Ryan Winchester (fungku)",
		"email": "fungku@gmail.com"
	}
  ],
  "require": {
    "php": ">=5.3.0",
    "ext-soap": "*",
    "ext-SimpleXML": "*",
    "ext-openssl": "*",
    "paragonie/random_compat": "^2.0"
  },
  "require-dev": {
    "phpspec/phpspec": "^2.4"
  },
  "autoload": {
    "psr-4": {
      "NetSuite\\": "src/"
    },
    "files": [
      "src/includes/functions.php"
    ]
  },
  "minimum-stability": "stable"
}
